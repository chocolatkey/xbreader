{
  "name": "xbreader",
  "version": "0.28.7",
  "description": "Fixed-layout publication reader for the web",
  "contributors": [
    "chocolatkey",
    "dasZGFz <daszgfz@protonmail.com> (https://das.zgfz.me)"
  ],
  "license": "MIT",
  "homepage": "https://github.com/chocolatkey/xbreader",
  "bugs": {
    "url": "https://github.com/chocolatkey/xbreader/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chocolatkey/xbreader.git"
  },
  "browser": "dist/xbreader-en.js",
  "scripts": {
    "start": "webpack-dev-server -d --config webpack.dev.js --watch",
    "build": "rm -rf dist && rm -rf stats && webpack -p --config webpack.prod.js",
    "build:analyze": "rm -rf dist && rm -rf stats && webpack -p --config webpack.prod.js --analyze",
    "push": "rm -rf dist/index.html && mv dist/index-en.html dist/index.html && surge dist/ xb.yrkz.me && mv dist/index.html dist/index-en.html",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore ."
  },
  "files": [
    "dist/!(*.json|logo.svg)"
  ],
  "keywords": [
    "webpub",
    "fixed-layout reader",
    "webpub reader"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/mithril": "^1.1.16",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "i18n-webpack-plugin": "^1.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mithril": "^2.0.0-rc.4",
    "mopt": "^5.1.1",
    "node-sass": "^4.12.0",
    "r2-shared-js": "^1.0.11",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "ta-json-x": "^2.5.0",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-fix-style-only-entries": "^0.2.1",
    "webpackbar": "^3.2.0"
  }
}
